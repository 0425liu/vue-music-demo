{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "js": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "restParams": true,
        "no-console": "off"
    }
}